{
	"$schema": "https://typedoc.org/schema.json",
	"categorizeByGroup": false,
	"cleanOutputDir": true,
	"entryPoints": ["packages/@lou.codes/*", "packages/*"],
	"entryPointStrategy": "packages",
	"excludeTags": ["@category"],
	"githubPages": false,
	"gitRevision": "main",
	"hideBreadcrumbs": true,
	"hideGenerator": true,
	"hideInPageTOC": true,
	"logLevel": "Warn",
	"name": "Lou's Code",
	"out": "./docs/.typedoc",
	"plugin": ["typedoc-plugin-markdown"]
}
